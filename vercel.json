{
  "version": 2,
  "framework": "flask",
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "crons": [
    {
      "path": "/api/cron/fetch?source=all&cron_token=${CRON_TOKEN}",
      "schedule": "0 */12 * * *"
    }
  ]
}